group Server
    storage: none
    source https://api.nuget.org/v3/index.json

    nuget FSharp.Core
    nuget Microsoft.AspNetCore.HttpsPolicy
    nuget SixLabors.ImageSharp
    nuget SQLProvider
    nuget Mysql.Data ~> 6 storage:packages
    nuget FSharp.Compiler.Tools storage:packages
    nuget Saturn
    nuget Fable.JsonConverter
    nuget Thoth.Json.Giraffe
    nuget Thoth.Json.Net

group Client
    storage: none
    source https://api.nuget.org/v3/index.json

    nuget Fable.Core
    nuget Fable.Elmish
    nuget Fable.Elmish.Debugger
    nuget Fable.Elmish.React
    nuget Fable.Elmish.HMR
    nuget Fable.React
    nuget Fulma
    nuget Fulma.Extensions

    clitool dotnet-fable

group Build
    source https://api.nuget.org/v3/index.json

    nuget FSharp.Core 4.3.4 // https://github.com/fsharp/FAKE/issues/2001
    nuget Fake.Core.Target
    nuget Fake.Core.Process
    nuget Fake.DotNet.Cli
    nuget Fake.IO.FileSystem